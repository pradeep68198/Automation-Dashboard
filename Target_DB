<%

String Type=(String)session.getAttribute("Type_Source"); 

if(Type.equalsIgnoreCase("No")){
Src_DB=request.getParameter("src_type");
session.setAttribute("Src_DB",Src_DB);
}


if(Src_DB.equalsIgnoreCase("Hive")){
	
	
	session.setAttribute("Src_Query_Hive",request.getParameter("src_query_hive"));
	session.setAttribute("Src_Username_Hive",request.getParameter("src_username_hive"));
	session.setAttribute("Src_Password_Hive",request.getParameter("src_password_hive"));
}

if(Src_DB.equalsIgnoreCase("SQL Server")){
	
	session.setAttribute("Src_Query_SQL",request.getParameter("src_query_sql"));
	session.setAttribute("Src_Username_SQL",request.getParameter("src_username_sql"));
	session.setAttribute("Src_Password_SQL",request.getParameter("src_password_sql"));
	session.setAttribute("Src_Server_SQL",request.getParameter("src_server_name_sql"));
	session.setAttribute("Src_DBname_SQL",request.getParameter("src_db_name_sql"));
}

if(Src_DB.equalsIgnoreCase("DB2")){
	
	session.setAttribute("Src_Query_DB2",request.getParameter("src_query_db2"));
	session.setAttribute("Src_Username_DB2",request.getParameter("src_username_db2"));
	session.setAttribute("Src_Password_DB2",request.getParameter("src_password_db2"));
	session.setAttribute("Src_DBname_DB2",request.getParameter("src_db_name_db2"));
	session.setAttribute("Src_Port_DB2",request.getParameter("src_port_db2"));
	session.setAttribute("Src_Host_DB2",request.getParameter("src_host_db2"));
}

if(Src_DB.equalsIgnoreCase("Netezza")){
	
	session.setAttribute("Src_Query_Netezza",request.getParameter("src_query_netezza"));
	session.setAttribute("Src_Username_Netezza",request.getParameter("src_username_netezza"));
	session.setAttribute("Src_Password_Netezza",request.getParameter("src_password_netezza"));
	session.setAttribute("Src_DBname_Netezza",request.getParameter("src_db_name_netezza"));
	session.setAttribute("Src_Server_Netezza",request.getParameter("src_server_name_netezza"));
}


session.setAttribute("Type_Target","No"); 


%>
 
