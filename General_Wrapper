public void html_update(String text) throws FileNotFoundException, UnsupportedEncodingException, InterruptedException {
	
	String value="<html>\r\n" + 
			"<head>\r\n" + 
			"<title>CVS E2E Automation</title>\r\n" + 
			"</head>\r\n" + 
			"<body><center><h1>CVS E2E Automation</h1><pre>                    \r\n" + 
			"\r\n" + 
			"\r\n" + 
			"\r\n" + 
			"\r\n" + 
			"\r\n" + 
			"\r\n" + 
			"</pre>\r\n" + 
			"<h2>%s .....</h2><br><br><img src=\""+Flow_Diagram+"\" alt=\"Architecture Flow\"></center></body></html>";
	
	String file = String.format(value,text);
	PrintWriter writer1 = new PrintWriter(Download_Path+"Result.html", "UTF-8");
	writer1.println(file);  
    writer1.close();
    String path=Download_Path+"Result.html";
    
    driver.get(path);
    Thread.sleep(2000);
	
}


public String Date_converter(String val) throws ParseException {
	String fin=null;
	
	SimpleDateFormat formatter = new SimpleDateFormat("mm/DD/yyyy");
	Date date1 = formatter.parse(val);
	SimpleDateFormat formatter1 = new SimpleDateFormat("yyyy-mm-dd");

	fin=formatter1.format(date1);
	
	
	return  fin;
	
	
}





    	 
